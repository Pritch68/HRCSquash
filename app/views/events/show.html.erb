<div class="row">
	<div class="col-lg-6 col-lg-offset-3">
		<div class="row">
      <div class="col-lg-12">
        <h4><%= @event.title %></h4>
      </div>
    </div>
    <div class="row">
        <div class="col-lg-12">Starts: <%= @event.startdate.strftime("%-d %b %Y") %></div>
      </div>
      <div class="row">
        <div class="col-lg-12">Ends: <%= @event.enddate.strftime("%-d %b %Y") %></div>
    </div>
    <div class="row">
      <div class="col-lg-12"><%= @event.body.html_safe %></div>
    </div>
	</div>
</div>